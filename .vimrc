set smartindent
set tabstop=2
set shiftwidth=2
set expandtab
nmap <silent> <c-k> :wincmd k<CR>
nmap <silent> <c-j> :wincmd j<CR>
nmap <silent> <c-h> :wincmd h<CR>
nmap <silent> <c-l> :wincmd l<CR>
colorscheme molokai
nnoremap <up> <nop>
nnoremap <down> <nop>
nnoremap <left> <nop>
nnoremap <right> <nop>
inoremap <up> <nop>
inoremap <down> <nop>
inoremap <left> <nop>
inoremap <right> <nop>
map zl :FufLine<CR>
map zb :FufBuffer<CR>
map zo :FufFile<CR>
map zc :FufCoverageFile<CR>
map :qq :q!


